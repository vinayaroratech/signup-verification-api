{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "UseInMemoryDatabase": true,
  "DBProvider": "Postgresql",
  "ExternalProviders": {
    "SendGrid": {
      "ApiKey": "/ContentAPI/SendGridApiKey",
      "SenderEmail": "info@example.com",
      "SenderName": "Info"
    }
  },
  "AppSettings": {
    "SecretKey": "MYSECRETSUPERSECRET",
    "Expiration": 2,
    "Issuer": "SampleApp",
    "Audience": "https://localhost"
  }
}
